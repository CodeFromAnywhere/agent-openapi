{
  "$schema": "https://raw.githubusercontent.com/CodeFromAnywhere/ActionSchema/main/schemas/action-schema.schema.json",

  "type": "object",
  "properties": {
    "created_at": {
      "type": "integer"
    },
    "description": { "type": "string", "nullable": true },
    "id": {
      "type": "string"
    },
    "instructions": {
      "type": "string",
      "nullable": true
    },
    "metadata": { "description": "Unknown metadata" },
    "model": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "nullable": true
    },
    "object": {
      "type": "string"
    },
    "response_format": {
      "oneOf": [
        { "type": "string" },
        {
          "type": "object",
          "properties": { "type": { "type": "string" } }
        }
      ],
      "nullable": true
    },
    "temperature": {
      "type": "number",
      "nullable": true
    },
    "tool_resources": {
      "description": "tools resources"
    },
    "tools": {
      "type": "array",
      "items": {},
      "minItems": 0
    },
    "top_p": {
      "type": "number",
      "nullable": true
    }
  },
  "required": [
    "created_at",
    "description",
    "id",
    "instructions",
    "metadata",
    "model",
    "name",
    "object",
    "tools"
  ]
}
